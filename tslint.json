{
  "defaultSeverity": "error",
  "extends": "tslint-config-airbnb",
  "jsRules": {},
  "rules": {
    "import-name": false,
    "class-methods-use-this": [
      0
    ],
    "no-continue": [
      0
    ],
    "comma-dangle": [
      1,
      "never"
    ],
    "trailing-comma": false,
    "spaced-comment": [
      0,
      "always"
    ],
    "no-restricted-syntax": [
      "error",
      "ForInStatement",
      "LabeledStatement",
      "WithStatement"
    ],
    "no-underscore-dangle": [
      "error",
      {
        "allowAfterThis": true
      }
    ],
    "no-await-in-loop": [
      0
    ],
    "max-line-length": [
      true,
      120
    ]
  },
  "rulesDirectory": []
}
